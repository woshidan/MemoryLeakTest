<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.woshidan.memoryreaktest">

    <application
        android:name=".TestApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".rarely_leak.BottomSheetActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".rarely_leak.RecyclerViewActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".rarely_leak.DialogActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".rarely_leak.NavigationViewActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".often_leak.StaticFieldLeakActivity"
            android:label="@string/title_activity_static_field_leak"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".often_leak.LongLivingHandlerActivity"
            android:label="@string/title_activity_long_living_handler"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".rarely_leak.RxJavaActivity"
            android:label="@string/title_activity_rx_java"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".often_leak.AsyncTaskWithLeakActivity"
            android:label="@string/title_activity_async_task_with_leak"
            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity
            android:name=".rarely_leak.AsyncTaskWithoutLeakActivity"
            android:label="@string/title_activity_async_task_without_leak"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
